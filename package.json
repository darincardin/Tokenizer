{
  "name": "react-widgets",
  "version": "3.0.0",
  "description": "A collection of react widgets",
  "keywords": ["react", "widget"],
  "author": "Darin Cardin",
  "scripts": {
      "start:form":      "cd form      && npm run start && cd ..",
      "start:stepbar":   "cd stepbar   && npm run start && cd ..",
      "start:tokenizer": "cd tokenizer && npm run start && cd ..",
      "build": "npm run build:form && npm run build:stepbar && npm run build:tokenizer",
      "build:form":      "cd form      && npm run build && cd ..",
      "build:stepbar":   "cd stepbar   && npm run build && cd ..",
      "build:tokenizer": "cd tokenizer && npm run build && cd .."
  },
