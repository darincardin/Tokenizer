{
  "name": "react-widgets",
  "version": "3.0.0",
  "author": "Darin Cardin",
  "description": "A collection of three react widgets: tokenizer, form, and stepbar",
  "keywords": [
    "react",
    "widget"
  ],
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "build": "webpack --env prod=true --mode production",
    "start:stepbar": "webpack-dev-server --open --config=webpack.config.stepbar.js",
    "start:tokenizer": "webpack-dev-server --open --config=webpack.config.tokenizer.js",
    "start:form": "webpack-dev-server --open --config=webpack.config.form.js"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^10.0.0",
    "bootstrap": "5.0",
    "browser-sync": "^2.26.7",
    "browserify": "^17.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^3.4.2",
    "del": "^5.1.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.0.4",
    "jquery": ">=1.9.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "react": "^16.13.1",
    "react-cookie": "^4.1.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^1.2.5",
    "react-lazyload": "^2.6.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "sass": "^1.55.0",
    "sass-loader": "^10",
    "scss": "^0.2.4",
    "style-loader": "^1.3.0",
    "uglify-save-license": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "vinyl": "^3.0.0",
